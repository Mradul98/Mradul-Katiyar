package Collection;

import java.util.ArrayList;
import java.util.List;

public class Collection_Practice {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		List<String> words=List.of("Mradul","Niket","Priyanshu","Himanshu");
		
		System.out.println(words);
		
		System.out.println(words.size());
		
		System.out.println(words.get(2));
		
		System.out.println(words.contains("Mradul"));
		
		
		System.out.println(words.isEmpty());
		
		System.out.println(words.indexOf("Himanshu"));
		
		//words.add("Anjali");// list are immutable we can not add the value in list in order to do that we need to use arraylist,vector,linkedlist
		
		ArrayList<String> wordsarraylist=new ArrayList<String>(words);
		
		wordsarraylist.add("Anjali");
		System.out.println(wordsarraylist);

	}

}
